{
  "globals": {
    "ga": true,
    "chrome": true
  },
  "plugins": [ "html", "json" ],
  "extends": "elemefe",
  "rules": {
    "no-restricted-globals": [ "error", "event", "fdescribe" ],
    "brace-style": [ "error", "stroustrup", { "allowSingleLine": true } ],
    "indent": "off",
    "operator-linebreak": "off"
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
